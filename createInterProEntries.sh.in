#!/bin/bash
# Parses the interpro entries

set -o pipefail -e
exec 3>&1

entries="http://ftp.ebi.ac.uk/pub/databases/interpro/entry.list"

lines="$(wget -q "$entries" -O -                               \
       | grep "^IPR"                                           \
       | sort                                                  \
       | nl -s "	"                                         \
       | tee >(cat >&3)                                        \
       )"